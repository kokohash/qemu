{
  "_id": "@xpack/logger@5.0.2",
  "_inBundle": true,
  "_location": "/xpm/@xpack/logger",
  "_phantomChildren": {},
  "_requiredBy": [
    "/xpm/@xpack/xpm-liquid"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "https://github.com/ilg-ul/"
  },
  "bugs": {
    "url": "https://github.com/xpack/logger-ts/issues/"
  },
  "bundleDependencies": [],
  "description": "A generic console logger class",
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@types/tap": "^15.0.0",
    "standard": "^16.0.3",
    "tap": "^15.0.6",
    "ts-node": "^9.1.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">10.0"
  },
  "homepage": "https://github.com/xpack/logger-ts/",
  "keywords": [
    "cli",
    "logger",
    "log",
    "console",
    "level",
    "silent",
    "error",
    "warn",
    "info",
    "verbose",
    "debug",
    "trace"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@xpack/logger",
  "peerDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpack/logger-ts.git"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "compile-watch": "tsc -watch -p ./",
    "fix": "ts-standard --fix src && standard --fix tests",
    "git-log": "git log --pretty='%cd * %h %s' --date=short",
    "install ": "npm install",
    "lint": "ts-standard src",
    "outdated": "npm outdated",
    "pack": "npm pack",
    "prepublish": "npm run compile && npm run lint",
    "pretest": "npm run compile && npm run lint",
    "tap": "tap --ts --reporter=spec --timeout 300 --no-coverage",
    "test": "npm run test-tap100 -s",
    "test-coverage": "tap --ts --coverage --reporter=classic --timeout 600 --no-check-coverage \"tests/tap/*.js\"",
    "test-tap": "tap --ts --reporter=classic --timeout 300 --no-coverage \"tests/tap/*.js\"",
    "test-tap100": "tap --ts --reporter=classic --timeout 300 --100 \"tests/tap/*.js\"",
    "update": "npm update",
    "version-minor": "npm version minor",
    "version-patch": "npm version patch"
  },
  "types": "./dist/index.d.ts",
  "version": "5.0.2"
}
